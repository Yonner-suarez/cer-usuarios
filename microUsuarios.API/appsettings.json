{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "conexion": "Server=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};User ID=${DB_USER};Password=${DB_PASSWORD}",
    "TipoPersona": {
      "Natural": "Natural",
      "Juridica": "Juridica"
    },
    "Token": {
      "PasswordHash": "${TOKEN_PASSWORD_HASH}",
      "SaltKey": "${TOKEN_SALT_KEY}",
      "VIKey": "${TOKEN_VI_KEY}",
      "Bearer": "${TOKEN_BEARER}",
      "Llave": "${TOKEN_KEY}",
      "Expiration": 60
    }
  }
}
